{
  "name": "@lvfang/react-simple-di",
  "version": "1.4.0",
  "description": "Simple Dependancy Injection Solution for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-simple-di"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "options": {
    "mocha": "--require scripts/mocha_runner lib/**/__tests__/**/*.js"
  },
  "scripts": {
    "prepare": ". ./scripts/prepublish.sh",
    "standard": "standard --fix",
    "testonly": "mocha $npm_package_options_mocha",
    "test": "npm run standard && npm run testonly"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "acorn": "^7.1.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-react": "^7.17.0",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.2",
    "react": "^16.x.x",
    "react-dom": "^16.x.x",
    "standard": "^14.3.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x.x",
    "react": "^16.x.x",
    "prop-types": "^15.x.x"
  },
  "dependencies": {
    "hoist-non-react-statics": "^3.3.1"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
}
