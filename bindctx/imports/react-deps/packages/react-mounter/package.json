    "mocha": "^7.0.0",
    "mock-require": "^3.0.3",
    "nodemon": "^2.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "sinon": "^8.0.4",
    "standard": "^14.3.1"
  },
  "peerDependencies": {
