{
  "name": "@lvfang/react-stubber",
  "version": "1.2.0",
  "description": "Simple but useful stubbing solution React",
  "repository": {
    "type": "git",
    "url": "https://github.com/kadirahq/react-stubber"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "prepare": ". ./.scripts/prepublish.sh",
    "standard": "standard --fix",
    "testonly": "mocha --require .scripts/mocha_runner src/**/tests/**/*.js",
    "test": "npm run standard && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js",
    "storybook": "start-storybook",
    "publish-storybook": "bash .scripts/publish_storybook.sh"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "@storybook/react": "^5.3.3",
    "acorn": "^7.1.0",
    "babel-loader": "^8.0.6",
    "chai": "^3.5.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "git-url-parse": "^11.1.2",
    "jsdom": "^16.0.0",
    "mocha": "^7.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "regenerator-runtime": "^0.13.3",
    "sinon": "^8.0.4",
    "standard": "^14.3.1",
    "webpack": "^4.41.5"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.x.x",
    "react": "^16.x.x"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
